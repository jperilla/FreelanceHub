       <![endif]-->
       <script type="text/javascript">

           var _gaq = _gaq || [];
           _gaq.push(['_setAccount', 'UA-37222723-1']);
           _gaq.push(['_trackPageview']);

           (function () {
               var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
               ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
               var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
           })();

        </script>
    </head>
    <body>
        <div id="wrapper">
            <header>
                <section class="sub-content">       
                    <nav>
                        <ul id="nav-list">
                            <li id="home">@Html.ActionLink("back to freelanceHub", "Index", "LandingPage")</li>
                        </ul>
                    </nav>
                </section>
            </header>
            
            <div id="content">
                <div class="sub-content">
                    @RenderBody()
                </div>
            </div>
            
            <footer>
                <div id="sub-content">
                    <p>Copyright ©  <a href="http://www.geekgirlsoftware.com">Geek Girl Software</a> 2012-2013<a href="http://www.privacychoice.org/policy/mobile?policy=3aba934cd01fbf1933ca804d37400984">Privacy Policy</a></p>
                    <ul id="social-media">
                        <li id="gmail"><a href="mailto:support@freelancehub.io" target="_blank"><img alt="gmail" src="/Content/images/gmail.png" /></a></li>
                        <li id="facebook"><a href="https://www.facebook.com/freelancehub.io" target="_blank"><img alt="facebook" src="/Content/images/facebook.png" /></a></li>
                        <li id="twitter"><a href="https://twitter.com/FreelanceHubio" target="_blank"><img alt="twitter" src="/Content/images/twitter.png" /></a></li>                   
                    </ul>
                </div>
            </footer>
